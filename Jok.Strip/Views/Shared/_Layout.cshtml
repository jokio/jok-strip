<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script>
        window.userid = '@ViewBag.UserID';
    </script>
    @Styles.Render("~/bundles/css")
    @Scripts.Render("~/bundles/js")
    <style>
        body{
            padding:0;
        }
    </style>
</head>
<body>
    <script src="~/Scripts/Common/require.js" data-main="/Scripts/Common/client"></script>
    @RenderBody()
</body>
</html>
